{
  "version": 2,
  "public": true,
  "buildCommand": "npm run build && cp -r api dist/ || xcopy api dist\\api /E /I",
  "outputDirectory": "dist",
  "env": {
    "UNIPAY_MERCHANT_ID": "5015191030581",
    "UNIPAY_API_KEY": "bc6f5073-6d1c-4abe-8456-1bb814077f6e"
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/biletebi",
      "destination": "/shop.html"
    },
    {
      "source": "/shekveta",
      "destination": "/simple-booking.html"
    },
    {
      "source": "/wesmdebi",
      "destination": "/terms.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}