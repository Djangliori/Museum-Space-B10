{
  "version": 2,
  "public": true,
  "buildCommand": null,
  "outputDirectory": "./",
  "env": {
    "UNIPAY_MERCHANT_ID": "5015191030581",
    "UNIPAY_API_KEY": "bc6f5073-6d1c-4abe-8456-1bb814077f6e"
  },
  "functions": {
    "api/unipay-create-order-production.js": {
      "runtime": "nodejs18.x"
    },
    "api/unipay-callback-production.js": {
      "runtime": "nodejs18.x"
    },
    "api/test-env.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/biletebi",
      "destination": "/shop.html"
    },
    {
      "source": "/shekveta",
      "destination": "/simple-booking.html"
    },
    {
      "source": "/wesmdebi",
      "destination": "/terms.html"
    },
    {
      "source": "/api/unipay-create-order-production",
      "destination": "/api/unipay-create-order-production.js"
    },
    {
      "source": "/api/unipay-callback-production", 
      "destination": "/api/unipay-callback-production.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}